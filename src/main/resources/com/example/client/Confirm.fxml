<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="96.0" prefWidth="220.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.client.ConfirmController">
    <children>
        <Label layoutX="37.0" layoutY="23.0" text="Xác nhận đăng xuất" textFill="RED">
            <font>
                <Font name="DejaVu Serif Bold" size="13.0" />
            </font>
        </Label>
        <Button fx:id="Cancel" layoutX="122.0" layoutY="52.0" mnemonicParsing="false" onMouseClicked="#onMouseClickedCancel" onMouseExited="#onMouseExitedCancel" onMouseMoved="#onMouseMovedCancel" prefHeight="30.0" prefWidth="60.0" style="-fx-background-color: rgb(255,38,76); -fx-background-radius: 20px;" text="Hủy">
            <font>
                <Font name="DejaVu Serif Bold" size="12.0" />
            </font>
        </Button>
        <Button fx:id="Ok" layoutX="37.0" layoutY="52.0" mnemonicParsing="false" onMouseClicked="#onMouseClickedOk" onMouseExited="#onMouseExitedOk" onMouseMoved="#onMouseMovedOk" prefHeight="30.0" prefWidth="60.0" style="-fx-background-color: rgba(20,218,103,1); -fx-background-radius: 20px;" text="OK">
            <font>
                <Font name="DejaVu Serif Bold" size="12.0" />
            </font>
        </Button>
    </children>
</AnchorPane>